<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" jcr:primaryType="nt:unstructured" jcr:title="Catalog" sling:resourceType="cq/gui/components/authoring/dialog" trackingFeature="learning:catalog">
    <content jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/tabs" maximized="{Boolean}false">
                <items jcr:primaryType="nt:unstructured">
                    <properties jcr:primaryType="nt:unstructured" jcr:title="Properties" sling:resourceType="granite/ui/components/coral/foundation/container" margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns" margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <showCatalogFilter 
                                            jcr:primaryType="nt:unstructured" 
                                            sling:resourceType="granite/ui/components/coral/foundation/form/switch" 
                                            fieldLabel="Show Catalog Filter" 
                                            name="./showCatalogFilter" 
                                            required="{Boolean}false" value="true" />
                                            <showFilters 
                                            jcr:primaryType="nt:unstructured" 
                                            sling:resourceType="granite/ui/components/coral/foundation/form/switch" 
                                            fieldLabel="Show Filters" 
                                            name="./showFilters" 
                                            required="{Boolean}false" value="true" />
                                            <showSearch 
                                            jcr:primaryType="nt:unstructured" 
                                            sling:resourceType="granite/ui/components/coral/foundation/form/switch" 
                                            fieldLabel="Show Search" 
                                            name="./showSearch" 
                                            required="{Boolean}false" value="true" />                                            
                                             <typeFilter 
                                            jcr:primaryType="nt:unstructured" 
                                            sling:resourceType="granite/ui/components/coral/foundation/form/switch" 
                                            fieldLabel="Show Type Filter" name="./typeFilter" 
                                            required="{Boolean}false" value="true" />
                                            <skillsFilter 
                                            jcr:primaryType="nt:unstructured" 
                                            sling:resourceType="granite/ui/components/coral/foundation/form/switch" 
                                            fieldLabel="Show Skills Filter" name="./skillsFilter" 
                                            required="{Boolean}false" value="true" />
                                            <formatFilter 
                                            jcr:primaryType="nt:unstructured" 
                                            sling:resourceType="granite/ui/components/coral/foundation/form/switch" 
                                            fieldLabel="Show Format Filter" name="./formatFilter" 
                                            required="{Boolean}false" value="true" />
                                            <durationFilter 
                                            jcr:primaryType="nt:unstructured" 
                                            sling:resourceType="granite/ui/components/coral/foundation/form/switch" 
                                            fieldLabel="Show Duration Filter" name="./durationFilter" 
                                            required="{Boolean}false" value="true" />
                                            <priceFilter 
                                            jcr:primaryType="nt:unstructured" 
                                            sling:resourceType="granite/ui/components/coral/foundation/form/switch" 
                                            fieldLabel="Show Price Filter" name="./priceFilter" 
                                            required="{Boolean}false" value="true" />
                                            <skillsLevelFilter 
                                            jcr:primaryType="nt:unstructured" 
                                            sling:resourceType="granite/ui/components/coral/foundation/form/switch" 
                                            fieldLabel="Show Skils Level Filter" name="./skillsLevelFilter" 
                                            required="{Boolean}false" value="true" />
                                            <statusFilter 
                                            jcr:primaryType="nt:unstructured" 
                                            sling:resourceType="granite/ui/components/coral/foundation/form/switch" 
                                            fieldLabel="Show Status Filter" name="./statusFilter" 
                                            required="{Boolean}false" value="true" />
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </properties>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>